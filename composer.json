{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.1",
        "binarycabin/laravel-uuid": "^1.0",
        "buzz/laravel-google-captcha": "^2.3",
        "code-lts/laravel-fcm": "^1.8",
        "consoletvs/charts": "6.*",
        "guzzlehttp/guzzle": "^7.2",
        "khaled.alshamaa/ar-php": "^6.3",
        "laravel/framework": "^10.10",
        "laravel/helpers": "^1.6",
        "laravel/sanctum": "^3.2",
        "laravel/telescope": "^4.17",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.2",
        "maatwebsite/excel": "^3.1",
        "maennchen/zipstream-php": "3.0.*",

        "mcamara/laravel-localization": "^1.8",
        "milon/barcode": "^10.0",
        "niklasravnsborg/laravel-pdf": "^4.1",
        "pusher/pusher-php-server": "^7.2",
        "rap2hpoutre/laravel-log-viewer": "*",
        "simplesoftwareio/simple-qrcode": "~4",
        "spatie/laravel-webhook-client": "^3.2",
        "spatie/laravel-webhook-server": "^3.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "kitloong/laravel-migrations-generator": "^6.11",
        "laravel/pint": "^1.14",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.8",
        "nunomaduro/phpinsights": "2.9.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],

        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "app/Liberary\\":"app/Liberary"

        }

    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}